	printf_s("\n *******");
	printf_s("\n*********");

	const char *napis;


	for (int i = 0; i < argc; i++)
	{
		/*for (int j = cols; j >0; j--) {
			argv[0] = " ";
		}*/
		for (int j = 0 ; j < argc; j++) {
